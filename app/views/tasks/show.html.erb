<p id="notice"><%= notice %></p>

<h1><%= t("views.tasks.show.title") %></h1>

<p>
  <strong><%= t("views.tasks.show.labels.title")%>:&nbsp;</strong>
  <%= @task.title %>
</p>

<p>
  <strong><%= t("views.tasks.show.labels.content")%>:&nbsp;</strong>
  <%= @task.content %>
</p>

<p>
  <strong><%= t("views.tasks.show.labels.created_at")%>:&nbsp;</strong>
  <%= @task.created_at %>
</p>

<%= link_to t("views.tasks.show.links.edit"), edit_task_path(@task), id: 'edit-task' %> |
<%= link_to t("views.tasks.show.links.back"), tasks_path, id: 'back' %>
