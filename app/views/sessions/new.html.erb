<%= form_with(url: new_session_path, local: true) do |form| %>
  <div>
    <%= form.label :email, 'メールアドレス' %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password, 'パスワード' %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit 'ログイン', id: 'create-session' %>
  </div>
<% end %>